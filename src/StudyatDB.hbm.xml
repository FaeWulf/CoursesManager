<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.StudyatDB" table="studyat" schema="coursesmanager">
        <composite-id class="com.model.StudyatDBPK">
            <key-property name="classId">
                <column name="class_id" sql-type="varchar(255)"/>
            </key-property>
            <key-property name="studentId">
                <column name="student_id" sql-type="int"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>